{% extends 'base.html.twig' %}

{% block stylesheets %}
<link rel="stylesheet" href="{{ asset('css/admin.css') }}">
{% endblock %}

{% block body %}

{# Bloc titre #}
<div class="container-fluid">
    <div class="row">
        <div class="blocTitle text-center col-xl-12">
            <h1>Cabinet Sages-Femmes Osny</h1>
            <h4><i class="fas fa-briefcase-medical"></i> Sages-Femmes diplomés d'état</h4>  
        </div>
    </div>
</div>

{# Formulaire d'inscription #}
<div class="container">
    <div class="jumbotron mb-5 mt-5">
        <h2 class="text-center mb-5">Formulaire d'inscription</h2>

        {{ form_start(form) }}
        {{ form_row(form.email) }}
        {{ form_row(form.password) }}
        {{ form_row(form.passwordConfirm) }}
         <button type="submit" class="btn btn-success form-control btn-block">Inscription !</button>
        {{ form_end(form) }}
    </div>
</div>

{# Footer #}

{{ include('cabinet/footer.html.twig') }}

{% endblock %}

